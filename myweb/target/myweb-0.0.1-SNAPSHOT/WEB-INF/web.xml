<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>cn.zhku.servlet.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addUser</servlet-name>
  	<display-name>addUser</display-name>
  	<description></description>
  	<servlet-class>cn.zhku.servlet.AddUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loadUsers</servlet-name>
  	<display-name>loadUsers</display-name>
  	<description></description>
  	<servlet-class>cn.zhku.servlet.LoadUsers</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>delUser</servlet-name>
  	<display-name>delUser</display-name>
  	<description></description>
  	<servlet-class>cn.zhku.servlet.DelUser</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addUser</servlet-name>
  	<url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loadUsers</servlet-name>
  	<url-pattern>/loadUsers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>delUser</servlet-name>
  	<url-pattern>/delUser</url-pattern>
  </servlet-mapping>
</web-app>
